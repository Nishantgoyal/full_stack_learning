<%- include ("partials/header.ejs") %>

    <% blogs.forEach(function(blog) { %>
        <div>
            <h3> <%= blog.title %> </h3>
            <img src="<%= blog.image %>">
            <span> <%= blog.created %> </span>
            <p> <%= blog.body %> </p>
        </div>
    <% }); %>

