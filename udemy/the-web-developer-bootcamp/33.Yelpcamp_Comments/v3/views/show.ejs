<%- include ("partials/header.ejs") %>

<h1> <%= campground.name %> </h1>
<img src="<%= campground.image %>">
<p> <%= campground.desc %> </p>

<div>
  <a href="/campgrounds"> View all Campgrounds </a>
</div>

<% campground.comments.forEach(function(comment) { %>
  <p> 
    <strong>
      <%= comment.author %> 
    </strong>
    - <%= comment.text %>
  </p>
<% }); %>

<%- include ("partials/footer.ejs") %>
